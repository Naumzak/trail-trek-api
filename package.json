{
  "name": "trail-trek-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "docker-test:restart": "docker-compose -f docker-compose.test.yml down && docker-compose -f docker-compose.test.yml up -d",
    "docker-test:stop": "docker-compose -f docker-compose.test.yml down -v",
    "pretest:e2e": "yarn docker-test:restart",
    "test:e2e": "NODE_ENV=test jest -i --no-cache --config jest-e2e.json --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/db/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/db/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/db/data-source.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "apollo-server-express": "^3.13.0",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^15.0.2",
    "graphql-upload-ts": "^2.1.2",
    "ioredis": "^5.4.1",
    "pg": "^8.11.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
